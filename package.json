{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "A Parcel, React, and TypeScript boilerplate to get up and running quick.",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html",
    "start": "parcel build/index.html",
    "build": "parcel build ./src/index.html --out-dir build",
    "format": "prettier \"src/**/*.{html,css,scss,js,ts,tsx}\" --write",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EleazarVillarreal/parcel-boilerplate.git"
  },
  "author": "Eleazar Villarreal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EleazarVillarreal/parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/EleazarVillarreal/parcel-boilerplate#readme",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
